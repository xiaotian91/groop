<h4> Public Timeline </h4>
<hr/>
<% @post_list.each do |post| %>
  <%#= debug post %>
  <% if post.groop_id == @groop.id && !post.content.nil? %>
  <div class="post">
    <div class="content">
     <%= post.content %>
    </div>
    <span style="font-size:small;">
     <%= post.posted %>
     <% unless post.user.nil? %>
       by <%= link_to(post.user.real_name, user_url(post.user)) %>
       <%= link_to("X", post_url(post.id), method: :delete, remote: true) %>
     <% end %>
        </span>
  </div>
  <hr/>
  <%end%>
<% end %>
