<h1> New Groop </h1>
<%= form_for(@groop) do |f| %>
  <% if @groop.errors.any? %>
  <div id="errorExplanation">
    <h2><%= pluralize(@groop.errors.count, "error") %> prohibited this user from being saved:</h2>
    <ul>
    <% @groop.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
  <% end %>
  <%= f.label :groop_name, "Groop Name" %> <%= f.text_field :groop_name %>
  <%= f.submit %>
<% end %>

<%= link_to "Back to list", groopings_url %>
